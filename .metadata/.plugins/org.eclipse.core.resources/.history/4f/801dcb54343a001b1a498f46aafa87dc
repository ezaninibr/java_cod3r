package fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		//(Fº - 32) x 5/9 = °C ***Fórmula para converter F° em C°
		
		final int fator = 32;
		final double divisor = 5/9.0;
		int farenheit;
		double celsius;
		
		farenheit = 63;
		
		celsius = (farenheit - 32) * divisor;
		
		System.out.println(celsius);
		
	}

}
