package fundamentos;

public class ConversaoTipoPrimitivoNumerico {
	public static void main(String[] args) {
		
		// implícita
		double a = 1;
		System.out.println(a); 
		
		// (tipo) na frente do valor = conversão explicita (CAST) - mas existe a possibilidade de truncamento pq o valor pode ser maior
		float b = (float) 1.1234567888888;
		System.out.println(b);
		
		int c = 340;
		byte d = (byte) c;
		System.out.println(d);
		
	}
}
